{
	"name": "revng-vscode-llvm-ir",
	"displayName": "revng-vscode-llvm-ir",
	"description": "Revng VSCode extension for LLVM IR",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "yarn run compile-web",
		"vscode:prepublish": "yarn run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"lint-staged": "lint-staged",
		"pretty": "prettier --config .prettierrc 'src/**/*.ts' --write",
		"format": "eslint src --fix --ext ts && prettier --config .prettierrc 'src/**/*.ts' --write",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@types/mocha": "^9.0.0",
		"@types/vscode": "^1.63.0",
		"@types/webpack-env": "^1.16.3",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-web": "^0.0.15",
		"assert": "^2.0.0",
		"eslint": "^8.6.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gts": "^3.1.0",
		"husky": "^7.0.4",
		"lint-staged": ">=10",
		"mocha": "^9.1.3",
		"prettier": "^2.5.1",
		"process": "^0.11.10",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1"
	},
	"lint-staged": {
		"*.(t|j)s": [
			"eslint --cache --fix",
			"prettier --write"
		]
	}
}
